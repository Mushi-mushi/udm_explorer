{
  "name": "metadata",
  "type": "Metadata",
  "description": "General information associated with a UDM event.",
  "children": [
    { "name": "event_timestamp", "type": "Timestamp", "keyFieldInfo": ["Indexing"], "description": "The GMT timestamp when the event was generated.", "logstashMapping": "date {\n\tmatch => [\"timestamp_field\", \"ISO8601\"]\n\ttarget => \"%%PATH%%\"\n}" },
    { "name": "event_type", "type": "Enum", "keyFieldInfo": ["Curated detections", "Indexing"], "description": "The event type.", "enumValues": ["GENERIC_EVENT", "NETWORK_CONNECTION", "PROCESS_LAUNCH", "USER_LOGIN"] },
    { "name": "vendor_name", "type": "String", "keyFieldInfo": ["Curated detections", "Indexing"], "description": "The name of the product vendor.", "logstashMapping": "mutate {\n\trename => { \"source_vendor_field\" => \"%%PATH%%\" }\n}" },
    { "name": "product_name", "type": "String", "keyFieldInfo": ["Curated detections", "Indexing"], "description": "The name of the product.", "logstashMapping": "mutate {\n\trename => { \"source_product_field\" => \"%%PATH%%\" }\n}" },
    { "name": "product_log_id", "type": "String", "keyFieldInfo": ["Threat hunting"], "description": "A vendor-specific event identifier." },
    { "name": "product_event_type", "type": "String", "keyFieldInfo": ["Curated detections"], "description": "A product-specific event name or type." },
    { "name": "product_deployment_id", "type": "String", "keyFieldInfo": ["Threat hunting"], "description": "The deployment identifier assigned by the vendor for a product deployment." },
    { "name": "description", "type": "String", "keyFieldInfo": ["Curated detections"], "description": "A human-readable description of the event." },
    { "name": "ingestion_labels", "type": "Label", "description": "User-configured ingestion metadata labels.", "children": [
      { "name": "key", "type": "String", "keyFieldInfo": ["Curated detections"] },
      { "name": "value", "type": "String", "keyFieldInfo": ["Curated detections"] }
    ]}
  ]
}